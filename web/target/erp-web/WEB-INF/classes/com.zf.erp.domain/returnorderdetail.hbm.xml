<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zf.erp.domain.Returnorderdetail" table="returnorderdetail" >
        <id name="uuid" >
            <generator class="native"/>
        </id>

        <property name="goodsuuid" /> 
        <property name="goodsname" /> 
        <property name="price" /> 
        <property name="num" /> 
        <property name="money" /> 
        <property name="endtime" /> 
        <property name="ender" /> 
        <property name="storeuuid" /> 
        <property name="state" /> 
        <!--<property name="ordersuuid" /> -->

        <many-to-one name="returnorders" column="ordersuuid" class="com.zf.erp.domain.Returnorders"></many-to-one>
    </class>
</hibernate-mapping>
